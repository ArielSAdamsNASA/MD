CLASS P0090 APID=0144, DESC="SCX CPU1 Memory Dwell App Commands"
!
  CMD  SCX_CPU1_MD_NOOP         FCTN=0, DESC="SCX CPU1 MD no-op command code"
!
  CMD  SCX_CPU1_MD_ResetCtrs    FCTN=1, DESC="SCX CPU1 MD reset counters command code"
!
  CMDS SCX_CPU1_MD_StartDwell   FCTN=2, DESC="SCX CPU1 MD start dwell command code"
      UI TableMask  DESC="table mask"
      UI Spare      STATIC, INVISIBLE, DEFAULT=0, DESC="Alignment padding"
  END
  CMDS SCX_CPU1_MD_StartDwell1   FCTN=2, DESC="SCX CPU1 MD start dwell table 1 command code"
      UI TableMask  STATIC, INVISIBLE, DEFAULT=1, DESC="table 1"
      UI Spare      STATIC, INVISIBLE, DEFAULT=0, DESC="Alignment padding"
  END
  CMDS SCX_CPU1_MD_StartDwell2   FCTN=2, DESC="SCX CPU1 MD start dwell table 2 command code"
      UI TableMask  STATIC, INVISIBLE, DEFAULT=2, DESC="table 2"
      UI Spare      STATIC, INVISIBLE, DEFAULT=0, DESC="Alignment padding"
  END
  CMDS SCX_CPU1_MD_StartDwell3   FCTN=2, DESC="SCX CPU1 MD start dwell table 3 command code"
      UI TableMask  STATIC, INVISIBLE, DEFAULT=4, DESC="table 3"
      UI Spare      STATIC, INVISIBLE, DEFAULT=0, DESC="Alignment padding"
  END
  CMDS SCX_CPU1_MD_StartDwell4   FCTN=2, DESC="SCX CPU1 MD start dwell table 4 command code"
      UI TableMask  STATIC, INVISIBLE, DEFAULT=8, DESC="table 4"
      UI Spare      STATIC, INVISIBLE, DEFAULT=0, DESC="Alignment padding"
  END
!
  CMDS SCX_CPU1_MD_StopDwell    FCTN=3, DESC="SCX CPU1 MD stop dwell command code"
      UI TableMask  DESC="table mask"
      UI Spare      STATIC, INVISIBLE, DEFAULT=0, DESC="Alignment padding"
  END
  CMDS SCX_CPU1_MD_StopDwell1    FCTN=3, DESC="SCX CPU1 MD stop dwell table 1 command code"
      UI TableMask  STATIC, INVISIBLE, DEFAULT=1, DESC="table 1"
      UI Spare      STATIC, INVISIBLE, DEFAULT=0, DESC="Alignment padding"
  END
  CMDS SCX_CPU1_MD_StopDwell2    FCTN=3, DESC="SCX CPU1 MD stop dwell table 1 command code"
      UI TableMask  STATIC, INVISIBLE, DEFAULT=2, DESC="table 2"
      UI Spare      STATIC, INVISIBLE, DEFAULT=0, DESC="Alignment padding"
  END
  CMDS SCX_CPU1_MD_StopDwell3    FCTN=3, DESC="SCX CPU1 MD stop dwell table 3 command code"
      UI TableMask  STATIC, INVISIBLE, DEFAULT=4, DESC="table 3"
      UI Spare      STATIC, INVISIBLE, DEFAULT=0, DESC="Alignment padding"
