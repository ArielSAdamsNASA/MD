#include "osconfig.h"
#include "cfe_mission_cfg.h"
#include "md_platform_cfg.h"

CLASS P0090 APID=0144, DESC="GC  Memory Manager App Commands"
!
  CMD  GC_MD_NOOP         FCTN=0, DESC="GC  MD no-op command code"
!
  CMD  GC_MD_ResetCtrs    FCTN=1, DESC="GC  MD reset counters command code"
!
  CMDS GC_MD_StartDwell   FCTN=2, DESC="GC  MD start dwell command code"
      UI TableMask  DESC="table mask"
  END
  CMDS GC_MD_StartDwell1   FCTN=2, DESC="GC  MD start dwell table 1 command code"
      UI TableMask  STATIC, INVISIBLE, DEFAULT=1, DESC="table 1"
  END
  CMDS GC_MD_StartDwell2   FCTN=2, DESC="GC  MD start dwell table 2 command code"
      UI TableMask  STATIC, INVISIBLE, DEFAULT=2, DESC="table 2"
  END
  CMDS GC_MD_StartDwell3   FCTN=2, DESC="GC  MD start dwell table 3 command code"
      UI TableMask  STATIC, INVISIBLE, DEFAULT=4, DESC="table 3"
  END
  CMDS GC_MD_StartDwell4   FCTN=2, DESC="GC  MD start dwell table 4 command code"
      UI TableMask  STATIC, INVISIBLE, DEFAULT=8, DESC="table 4"
  END

!
  CMDS GC_MD_StopDwell    FCTN=3, DESC="GC  MD stop dwell command code"
      UI TableMask  DESC="table mask"
  END
  CMDS GC_MD_StopDwell1    FCTN=3, DESC="GC  MD stop dwell table 1 command code"
      UI TableMask  STATIC, INVISIBLE, DEFAULT=1, DESC="table 1"
  END
  CMDS GC_MD_StopDwell2    FCTN=3, DESC="GC  MD stop dwell table 1 command code"
      UI TableMask  STATIC, INVISIBLE, DEFAULT=2, DESC="table 2"
  END
  CMDS GC_MD_StopDwell3    FCTN=3, DESC="GC  MD stop dwell table 3 command code"
      UI TableMask  STATIC, INVISIBLE, DEFAULT=4, DESC="table 3"
  END
  CMDS GC_MD_StopDwell4    FCTN=3, DESC="GC  MD stop dwell table 4 command code"
      UI TableMask  STATIC ,INVISIBLE, DEFAULT=8, DESC="table 4"
  END

!
  CMDS GC_MD_JamDwell     FCTN=4, DESC="GC  MD jam dwell command code"
      UI TableId RANGE=(1,MD_NUM_DWELL_TABLES+1), DESC="id of the table"
      UI EntryId RANGE=(1,MD_DWELL_TABLE_SIZE), DESC="dwell table entry index"
      UI FieldLength RANGE=(0,4), DESC="length of dwell field"
      UI DwellDelay UNITS=Counts, DESC="dwell delay"
      ULI Offset   DESC="Offset from symbol or absolute address"
      char SymName[OS_MAX_SYM_LEN] DESC="Symbol Name of address to write"
  END
!
END       !END PACKET
!
