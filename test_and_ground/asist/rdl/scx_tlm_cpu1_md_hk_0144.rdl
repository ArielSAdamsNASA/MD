#include "md_platform_cfg.h"

PACKET P0090 APID=0144, DESC="SCX CPU1 MD Telemetry Data Packet",
                        STALE = 36
#include "ccsds_header.rdl"
!
  UB   SCX_CPU1_MD_CMDEC       DESC="SCX CPU1 MD Command Error Counter",
                               UNITS=Counts, YH=1, DOLIMIT
!
  UB   SCX_CPU1_MD_CMDPC       DESC="SCX CPU1 MD Command Processed Counter",
                               UNITS=Counts
!
  UNION SCX_CPU1_MD_MASK	DESC="SCX CPU1 MD Enable Status for each table"
      UI   SCX_CPU1_MD_EnableMask  DESC="SCX CPU1 MD Enable Mask"
      UI   SCX_CPU1_MD_Tblstat1 MASK= %x0001, DESC= "MD Table 1 Enable/Disable status",
                                DISCRETE, DRANGE =(0,1), DLABEL=("DIS","ENA")
      UI   SCX_CPU1_MD_Tblstat2 MASK= %x0002, DESC= "MD Table 2 Enable/Disable status",
                                DISCRETE, DRANGE =(0,1), DLABEL=("DIS","ENA")
      UI   SCX_CPU1_MD_Tblstat3 MASK= %x0004, DESC= "MD Table 3 Enable/Disable status",
                                DISCRETE, DRANGE =(0,1), DLABEL=("DIS","ENA")
      UI   SCX_CPU1_MD_Tblstat4 MASK= %x0008, DESC= "MD Table 4 Enable/Disable status",
                                DISCRETE, DRANGE =(0,1), DLABEL=("DIS","ENA")
  END

!
  UI   SCX_CPU1_MD_AddrCnt[MD_NUM_DWELL_TABLES] DESC="SCX CPU1 MD number of dwell addresses for each table"
!
  UI   SCX_CPU1_MD_Rate[MD_NUM_DWELL_TABLES] DESC="SCX CPU1 MD number of delay counts for each table"
!
  UI   SCX_CPU1_MD_DataSize[MD_NUM_DWELL_TABLES] DESC="SCX CPU1 MD number of bytes of data specified for each table"
!
  UI   SCX_CPU1_MD_DwPktOffset[MD_NUM_DWELL_TABLES] DESC="SCX CPU1 MD current write offset within dwell pkt data region for each table"
!
  UI   SCX_CPU1_MD_DwTblEntry[MD_NUM_DWELL_TABLES] DESC="SCX CPU1 MD next dwell table entry to be processed for each table"
!
  UI   SCX_CPU1_MD_CountDown[MD_NUM_DWELL_TABLES] DESC="SCX CPU1 current value of countdown timer for each table"
!
!  END                !END APPEND RECORD FUNCTION
