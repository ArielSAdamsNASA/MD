#include "osconfig.h"
#include "md_platform_cfg.h"

PACKET P00BB APID=0187, DESC="GC  MD Telemetry Data Packet",
                        STALE = 130
#include "ccsds_header.rdl"
!
  UB   GC_MD_CMDEC       DESC="GC  MD Command Error Counter",
                               UNITS=Counts, YH=1, DOLIMIT
!
  UB   GC_MD_CMDPC       DESC="GC  MD Command Processed Counter",
                               UNITS=Counts
!
  UNION MD_Mask          DESC=" GC MD Enable status for each table"
      UI   GC_MD_EnableMask  DESC="GC  MD Enable Mask"
      UI   GC_MD_Tblstat1 MASK= %x0001, DESC= "MD Table 1 Enable/Disable status", 
                                       DISCRETE, DRANGE =(0,1), DLABEL=("DIS","ENA")
      UI   GC_MD_Tblstat2 MASK= %x0002, DESC= "MD Table 2 Enable/Disable status", 
                                       DISCRETE, DRANGE =(0,1), DLABEL=("DIS","ENA")
      UI   GC_MD_Tblstat3 MASK= %x0004, DESC= "MD Table 3 Enable/Disable status", 
                                       DISCRETE, DRANGE =(0,1), DLABEL=("DIS","ENA")
      UI   GC_MD_Tblstat4 MASK= %x0008, DESC= "MD Table 4 Enable/Disable status", 
                                       DISCRETE, DRANGE =(0,1), DLABEL=("DIS","ENA")

  END
!
  UI   GC_MD_AddrCnt[MD_NUM_DWELL_TABLES] DESC="GC  MD number of dwell addresses for each table"
!
  UI   GC_MD_Rate[MD_NUM_DWELL_TABLES] DESC="GC  MD number of delay counts for each table"
!
  UI   GC_MD_DataSize[MD_NUM_DWELL_TABLES] DESC="GC  MD number of bytes of data specified for each table"
!
  UI   GC_MD_DwPktOffset[MD_NUM_DWELL_TABLES] DESC="GC  MD current write offset within dwell pkt data region for each table"
!
  UI   GC_MD_DwTblEntry[MD_NUM_DWELL_TABLES] DESC="GC  MD next dwell table entry to be processed for each table"
!
  UI   GC_MD_CountDown[MD_NUM_DWELL_TABLES] DESC="GC  current value of countdown timer for each table"
!
!  END                !END APPEND RECORD FUNCTION
!
END
